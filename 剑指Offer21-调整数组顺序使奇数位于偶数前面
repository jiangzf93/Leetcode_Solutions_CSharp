public class Solution {
    public int[] Exchange(int[] nums) {
        int start=0;
        int end=nums.Length-1;
        while(start<end){
            while(start<end&&nums[start]%2==1) start++;
            while(start<end&&nums[end]%2==0) end--;
            if(start<end){
                int temp=nums[start];
                nums[start]=nums[end];
                nums[end]=temp;
            }
        }
        return nums;
    }
}
